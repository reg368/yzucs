<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.answer_record.model.Answer_recordVO" table="YZU_ANSWER_RECORD" >
			
		<id name="r_id" column="r_id">
			<generator class="sequence">
				<param name="sequence">yzu_answer_record_seq</param>
			</generator>
		</id>
			<property name="r_userId" column="r_userId" />
			<property name="r_questionId" column="r_questionId" />	
			<property name="r_user_login_count" column="r_user_login_count" />
			<property name="r_correct_count" column="r_correct_count" />
			<property name="r_incorrect_count" column="r_incorrect_count" />
			<property name="r_user_joindate" column="r_uesr_joindate" insert="false" update="false"/>
			<property name="r_user_update_date" column="r_user_update_date" />	
	</class>
</hibernate-mapping>