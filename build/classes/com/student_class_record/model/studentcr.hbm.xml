<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.student_class_record.model.StudentCRVO" table="yzu_student_class_record" >
			
		<id name="cr_id" column="cr_id">
			<generator class="sequence">
				<param name="sequence">yzu_student_cr_seq</param>
			</generator>
		</id>
			<property name="cr_class_id" column="cr_class_id" />
			<property name="cr_student_id" column="cr_student_id" />
			<property name="cr_joindate" column="cr_joindate" insert="false"  update="false"/>			
	</class>
</hibernate-mapping>