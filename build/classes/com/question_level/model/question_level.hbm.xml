<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.question_level.model.Question_levelVO" table="yzu_quesiton_level" >
			
			<id name="l_id" column="l_id">
				 <generator class="sequence">
					<param name="sequence">yzu_question_level_seq</param>
				 </generator>
			</id>
			
			<property name="l_level" column="l_level" />
			<property name="l_joindate" column="l_joindate" insert="false" />
			<property name="l_group_id" column="l_group_id" />
	
	</class>
</hibernate-mapping>