<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.question_student_access.model.Q_student_accessVO" table="yzu_question_student_accesss" >
			
			<id name="access_id" column="access_id">
				 <generator class="sequence">
					<param name="sequence">yzu_q_s_access_seq</param>
				 </generator>
			</id>
			
			<property name="access_user_id" column="access_user_id" />
			<property name="access_question_group_id" column="access_question_group_id" />
	
	</class>
</hibernate-mapping>