<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.user.model.UserVO" table="YZU_USER" >
			
			<id name="user_id" column="user_id">
				 <generator class="com.user.model.UserSequenceGenerator" ></generator>
			</id>
			
			<property name="user_name" column="user_name" />
			<property name="user_gender" column="user_gender" />
			<property name="user_age" column="user_age" />
			<property name="user_grade" column="user_grade" />
			<property name="user_character_image" column="user_character_image" />
			<property name="user_pet_image" column="user_pet_image" />
			<property name="user_pet_name" column="user_pet_name" />
			<property name="user_login_id" column="user_login_id" />
			<property name="user_password" column="user_password" />
			<property name="user_joindate" column="user_joindate" insert="false" update="false"/>
			<property name="user_cImage_type" column="user_cImage_type" />
			<property name="user_pImage_type" column="user_pImage_type" />
			<property name="user_login_count" column="user_login_count" />
			<property name="user_group_id" column="user_group_id" />
			<property name="user_teacher_id" column="user_teacher_id" />
			
	</class>
</hibernate-mapping>