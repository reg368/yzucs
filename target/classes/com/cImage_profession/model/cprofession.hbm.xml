<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.cImage_profession.model.CProfessionVO" table="yzu_cimage_profession" >
			
		<id name="cprofession_id" column="cprofession_id">
			<generator class="sequence">
				<param name="sequence">cprofession_id_seq</param>
			</generator>
		</id>
			
			<property name="cprofession_title" column="cprofession_title" />
			<property name="cprofession_joindate" column="cprofession_joindate" insert="false"/>		
	</class>
</hibernate-mapping>