<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.QL.model.QLVO" table="yzu_QL_mapping" >
			
		<id name="ql_id" column="ql_id">
			<generator class="sequence">
				<param name="sequence">yzu_QL_mapping_seq</param>
			</generator>
		</id>
			<property name="l_id" column="l_id" />
			<property name="q_id" column="q_id" />
			<property name="g_id" column="g_id" />		
	</class>
</hibernate-mapping>