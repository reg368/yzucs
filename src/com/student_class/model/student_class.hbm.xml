<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.student_class.model.StudentClassVO" table="yzu_student_class" >
			
		<id name="c_id" column="c_id">
			<generator class="sequence">
				<param name="sequence">yzu_student_class_seq</param>
			</generator>
		</id>
			<property name="c_name" column="c_name" />
			<property name="c_teacher_id" column="c_teacher_id" />
			<property name="c_joindate" column="c_joindate" insert="false"  update="false"/>			
	</class>
</hibernate-mapping>