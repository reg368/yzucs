<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.cImage_mood.model.CMoodVO" table="yzu_cimage_mood" >
			
		<id name="cMood_id" column="cMood_id">
			<generator class="sequence">
				<param name="sequence">yzu_cimage_mood_seq</param>
			</generator>
		</id>
			
			<property name="cMood_title" column="cMood_title" />
			<property name="cMood_joindate" column="cMood_joindate" insert="false"/>		
	</class>
</hibernate-mapping>