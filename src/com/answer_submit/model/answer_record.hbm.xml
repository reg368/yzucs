<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.answer_submit.model.Answer_submitVO" table="yzu_answer_submit" >
			
		<id name="s_id" column="s_id">
			<generator class="sequence">
				<param name="sequence">yzu_answer_submit_seq</param>
			</generator>
		</id>
			<property name="r_id" column="r_id" />
			<property name="a_id" column="a_id" />	
			<property name="s_joindate" column="s_joindate" insert="false" />
	</class>
</hibernate-mapping>