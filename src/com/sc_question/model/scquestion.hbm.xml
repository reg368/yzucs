<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sc_question.model.SCQuestionVO" table="yzu_s_class_question" >
			
		<id name="qc_id" column="qc_id">
			<generator class="sequence">
				<param name="sequence">yzu_s_class_question_seq</param>
			</generator>
		</id>
			<property name="group_id" column="group_id" />
			<property name="class_id" column="class_id" />			
	</class>
</hibernate-mapping>